{
    "rust-analyzer.cargo.features": [
        "full"
    ],
    "files.exclude": {
        "**/__pycache__": true
    },
    "workbench.editor.revealIfOpen": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoImportCompletions": true,
    "python.terminal.activateEnvironment": false,
    "black-formatter.args": [
        "--line-length",
        "150"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
    },
    "python.testing.pytestArgs": [
        "bindings/python/tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "cSpell.words": [
        "asctime",
        "atexit",
        "brkn",
        "builtins",
        "busywait",
        "byteserde",
        "canonicalize",
        "chrono",
        "classattr",
        "clbk",
        "clippy",
        "cogapp",
        "dunder",
        "dylib",
        "endofday",
        "freethreaded",
        "froms",
        "getattr",
        "hbeat",
        "inittab",
        "isinstance",
        "kwargs",
        "levelname",
        "libpython",
        "maturin",
        "micromamba",
        "nanos",
        "nbbo",
        "nextest",
        "NNBO",
        "nonblocking",
        "NOTSET",
        "popd",
        "pushd",
        "pyclass",
        "pyfunction",
        "pymethods",
        "pymodule",
        "pypi",
        "pytest",
        "qbbo",
        "recver",
        "Recvers",
        "repr",
        "riskless",
        "rpath",
        "rpii",
        "rstd",
        "rustc",
        "RUSTDOCFLAGS",
        "rustfmt",
        "rustup",
        "sdata",
        "setattr",
        "Softstream",
        "soupbintcp",
        "startofday",
        "traceback",
        "tshark",
        "udata",
        "unsequenced",
        "workdir",
        "wouldblock"
    ],
}